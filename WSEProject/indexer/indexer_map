border=\"0\" cellspacing=\"0\" cellpadding=\"4\" bgcolor=\"#eeeeee\"> \n   <tbody>\n    <tr> \n     <td align=\"center\"> \n      <!-- OAS CODE BEGIN --> <script language=\"JavaScript\" type=\"text/javascript\" src=\"http://www.signonsandiego.com/scripts/oas_topright.js\"></script> \n      <!-- OAS CODE END --> </td> \n    </tr> \n   </tbody>\n  </table> \n  <!-- END LEADERBOARD AD --> \n  <!-- SOSD HEADER END --> \n  <!-- BEGIN MIDDLE SECTION TABLE ----------> \n  <link href=\"/styles/rightrailstyle.css\" rel=\"stylesheet\">\n  <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"750\">\n   <tbody>\n    <tr>\n     <td valign=\"top\"> \n      <!-- BEGIN LEFT NAV COLUMN ----------> \n      <table width=\"130\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\" align=\"left\"> \n       <tbody>\n        <tr valign=\"top\"> \n         <td width=\"130\" align=\"left\"> \n          <table width=\"130\" cellpadding=\"2\" cellspacing=\"1\" border=\"0\" bgcolor=\"#666666\"> \n           <tbody>\n            <tr bgcolor=\"#000066\">\n             <td><a href=\"http://www.signonsandiego.com/news/politics/index.html\"><img src=\"http://www.signonsandiego.com/news/politics/images/election/election2002_logo.gif\" width=\"130\" height=\"130\" border=\"0\" alt=\"\"></a></td>\n            </tr> \n            <!-- News ----------> \n            <tr bgcolor=\"#707070\">\n             <td><a class=\"plainlinkhead\" href=\"/index.html\"><b>&nbsp;News</b></a></td>\n            </tr> \n            <tr bgcolor=\"#000066\">\n             <td><a class=\"plainlink\" href=\"/news/metro/index.html\"><b>&nbsp;Metro |</b></a><a href=\"/news/metro/index.html#latestnews\" class=\"plainlink\"><b> Latest News</b></a></td>\n            </tr> \n            <tr bgcolor=\"#000066\">\n             <td><a class=\"plainlink\" href=\"/news/northcounty/index.html\"><b>&nbsp;North County</b></a></td>\n            </tr> \n            <tr bgcolor=\"#000066\">\n             <td><a class=\"plainlink\" hre